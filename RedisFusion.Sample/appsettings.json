{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RedisFusionConfigurations": {
    "IsEnabled": true,
    "ConnectionString": "localhost:6379",
    "InstanceName": "MyCollection",
    "ExpirationTimeSpanInMinutes": 5,
    "AcceptableLanguages": [
      "en-US",
      "en",
      "ar-JO",
      "ar",
      "fr-FR",
      "es-ES"
    ]
  },
  "AllowedHosts": "*"
}
